# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here

# Scraper Base URLs
SAMEHADAKU_BASE_URL=https://v1.samehadaku.how
ANIMASU_BASE_URL=https://v0.animasu.app

# Logging Configuration
# LOG_LEVEL options: DEBUG, INFO, WARN, ERROR, SILENT
# DEBUG: All logs + performance timing + detailed data
# INFO: Standard logs + performance timing (default)
# WARN: Warnings and errors only
# ERROR: Errors only
# SILENT: No logs
LOG_LEVEL=INFO

# GitHub Action Configuration
# Personal Access Token (PAT) with repo scope for triggering GitHub Actions
GITHUB_ACTION_TOKEN=ghp_your_github_token_here
# Repository where the worker action is hosted (format: username/repo-name)
GITHUB_ACTION_REPO=your-username/github-action-worker

# Cloudflare Workers Configuration
# Cloudflare Workers video proxy URL (for streaming)
WORKER_VIDEO_PROXY_URL=https://anime-video-proxy.your-subdomain.workers.dev

# Primary Storage Account (for client response)
# Pilih storage account mana yang akan ditampilkan di response API
# Jika account-1 down, ganti ke account-2, account-3, dst
# Format: storage-account-1, storage-account-2, ..., storage-account-10
PRIMARY_STORAGE_ACCOUNT=storage-account-1
